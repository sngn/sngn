{
  "name": "@sngn/js-helpers",
  "version": "0.1.0",
  "description": "javascript/typescript helpers library",
  "keywords": [
    "javascript",
    "typescript"
  ],
  "author": "Maximilian Stejskal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sngn/sngn.git",
    "directory": "pkgs/js-helpers"
  },
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "typings": "./dist/typings",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.mjs"
    },
    "./testFunctions": {
      "import": "./dist/esm/testFunctions/index.mjs",
      "require": "./dist/cjs/testFunctions/index.cjs",
      "default": "./dist/esm/testFunctions/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "dependencies": {
  },
  "devDependencies": {
    "esbuild": "^0.17.6",
    "esbuild-node-externals": "^1.6.0",
    "esbuild-plugin-d.ts": "^1.1.0",
    "shx": "^0.3.4",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "node build.js",
    "cbuild": "npm run clean && npm run build",
    "clean": "shx rm -f /tmp/@sngn/js-helpers/.esbuild/.tsbuildinfo; shx rm -rf ./dist/*",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
